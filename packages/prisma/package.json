{
	"name": "@doggo/prisma",
	"version": "0.1.1",
	"scripts": {
		"build": "tsup index.ts --dts",
		"dev": "tsup index.ts --watch --dts",
		"clean": "rm -rf .turbo && rm -rf node_modules",
		"db-deploy": "prisma migrate deploy",
		"db-migrate": "prisma migrate dev",
		"db-format": "prisma format",
		"db-nuke": "prisma migrate reset",
		"db-studio": "prisma studio",
		"dx": "prisma db push && prisma studio",
		"dx:seed": "prisma db push --force-reset && ts-node ./seed.ts",
		"generate-schemas": "prisma generate"
	},
	"devDependencies": {
		"@doggo/config": "*",
		"prisma": "3.9.2",
		"ts-node": "10.5.0",
		"typescript": "4.5.5",
		"tsup": "5.12.0"
	},
	"dependencies": {
		"@prisma/client": "3.9.2"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/index.js"
	],
	"prisma": {
		"seed": "ts-node ./seed.ts"
	}
}
