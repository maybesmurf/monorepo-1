{
	"scripts": {
		"dev": "prisma migrate dev && ts-node-dev -r tsconfig-paths/register --exit-child --ignore-watch=node_modules src/index.ts & prisma studio",
		"build": "rm -rf build && prisma generate && tsc -p tsconfig.json",
		"start": "node ./build/src/index.js && prisma migrate deploy",
		"test": "jest --watchAll",
		"ci:test": "jest"
	},
	"_moduleAliases": {
		"@Utils": "build/src/utils",
		"@Libs": "build/src/libs",
		"@v1": "build/src/v1"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@faker-js/faker": "^5.5.3",
		"@prisma/client": "^3.8.1",
		"@sentry/node": "^6.17.3",
		"@sentry/tracing": "^6.16.1",
		"@types/cors": "^2.8.12",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.2",
		"firebase-admin": "^10.0.1",
		"module-alias": "^2.2.2",
		"nodemon": "^2.0.15",
		"prisma": "^3.7.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.5.5"
	},
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/faker": "^6.6.9",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^17.0.8",
		"tsconfig-paths": "^3.12.0"
	}
}
