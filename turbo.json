{
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build",
				"$DATABASE_URL",
				"$IS_TESTING",
				"$IS_DEV",
				"$NEXT_PUBLIC_API_PATH",
				"$NEXT_PUBLIC_IS_DEV",
				"$NEXT_PUBLIC_IS_TESTING",
				"$NEXT_PUBLIC_SENTRY_DSN",
				"$POSTGRES_DB",
				"$POSTGRES_PASSWORD",
				"$POSTGRES_USER",
				"$SENTRY_AUTH_TOKEN",
				"$SENTRY_DSN",
				"$SENTRY_ORG",
				"$SENTRY_PROJECT"
			],
			"outputs": ["dist/**", ".next/**", "build/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": ["coverage/**"]
		},

		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"type-check": {
			"outputs": []
		},
		"prisma:generate": {
			"cache": false
		}
	}
}
