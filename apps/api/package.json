{
	"name": "api",
	"version": "0.1.1",
	"type": "commonjs",
	"scripts": {
		"build": "rm -rf dist && tsc -p tsconfig.json",
		"compile": "tsc && node ./dist/src/index.js || exit 1",
		"dev": "nodemon -e ts",
		"start": "node ./dist/src/index.js",
		"test": "jest --coverage",
		"test:watch": "jest --watch --coverage",
		"lint": "eslint \"**/*.ts\" --ignore-pattern node_modules/",
		"clean": "rm -rf .turbo && rm -rf coverage && rm -rf dist && rm -rf node_modules",
		"type-check": "tsc --pretty --noEmit",
		"ci:test": "jest .*\\.unit\\.test\\.ts",
		"prisma:generate": "prisma generate --schema=../../packages/prisma/schema.prisma"
	},
	"_moduleAliases": {
		"@Root": "dist/src",
		"@Utils": "dist/src/utils",
		"@Libs": "dist/src/libs",
		"@v1": "dist/src/v1",
		"@Prisma": "dist/src/libs/prisma/index"
	},
	"dependencies": {
		"@doggo/logger": "*",
		"@doggo/ui": "*",
		"@faker-js/faker": "5.5.3",
		"@prisma/client": "3.9.2",
		"@sentry/node": "6.17.3",
		"@sentry/tracing": "6.16.1",
		"@typescript-eslint/eslint-plugin": "5.12.1",
		"cors": "2.8.5",
		"cookie-parser": "1.4.6",
		"cross-env": "7.0.3",
		"dotenv": "10.0.0",
		"eslint-plugin-jest": "26.1.1",
		"express": "4.17.2",
		"firebase-admin": "10.0.1",
		"joi": "17.6.0",
		"module-alias": "2.2.2",
		"nodemon": "2.0.15",
		"typescript": "4.5.5"
	},
	"devDependencies": {
		"@babel/core": "7.16.12",
		"@babel/plugin-transform-modules-commonjs": "7.16.8",
		"@babel/preset-typescript": "7.16.7",
		"@doggo/config": "*",
		"@types/cors": "2.8.12",
		"@types/cookie-parser": "1.4.2",
		"@types/express": "4.17.13",
		"@types/faker": "6.6.9",
		"@types/jest": "27.4.0",
		"@types/module-alias": "2.0.1",
		"@types/node": "17.0.8",
		"@types/supertest": "2.0.11",
		"@typescript-eslint/parser": "5.12.1",
		"babel-jest": "27.4.6",
		"eslint": "8.10.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-plugin-only-warn": "1.0.3",
		"prisma": "3.9.2",
		"jest": "27.4.7",
		"supertest": "6.2.2",
		"ts-jest": "27.1.3",
		"ts-node": "10.4.0",
		"tsconfig-paths": "3.12.0"
	}
}
