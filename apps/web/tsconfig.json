{
	"extends": "@doggo/config/tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@Firebase/*": ["services/firebase/*"],
			"@Utils/*": ["utils/*"],
			"@test-utils": ["utils/test-utils"],
			"@Components/*": ["components/*"],
			"@Styles/*": ["styles/*"],
			"@Hooks/*": ["hooks/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
