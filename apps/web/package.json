		"@mantine/dates": "3.6.9",
		"@mantine/hooks": "3.6.9",
		"@mantine/next": "3.6.9",
		"@mantine/rte": "3.6.9",
		"@testing-library/jest-dom": "5.16.2",
		"@testing-library/react": "12.1.2",
		"@types/node": "17.0.0",
		"@types/react": "17.0.37",
		"babel-jest": "27.5.1",
		"eslint": "8.8.0",
		"eslint-config-next": "12.0.7",
		"prisma": "3.9.2",
		"jest": "27.5.1",
		"ts-jest": "27.1.3",
		"typescript": "4.5.4"
	},
	"lint-staged": {
		"apps/**/*.{js,ts,jsx,tsx}": [
