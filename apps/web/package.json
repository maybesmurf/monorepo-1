{
	"name": "web",
	"version": "0.1.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"serve": "next start",
		"lint": "next lint",
		"tsc": "tsc --noEmit --incremental false",
