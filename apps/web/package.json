	},
	"dependencies": {
		"@doggo/config": "*",
		"@doggo/logger": "*",
		"@doggo/types": "*",
		"@mantine/core": "3.6.9",
		"@mantine/dates": "3.6.9",
		"@mantine/hooks": "3.6.9",
		"@mantine/next": "3.6.9",
		"@mantine/rte": "3.6.9",
		"@next-auth/prisma-adapter": "1.0.1",
		"@prisma/client": "3.9.2",
		"@sentry/nextjs": "6.17.3",
		"date-fns": "2.28.0",
		"firebase": "9.6.5",
		"msw": "0.36.8",
		"next": "12.1.0",
		"next-auth": "4.2.1",
		"next-connect": "0.11.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-hook-form": "7.26.1",
